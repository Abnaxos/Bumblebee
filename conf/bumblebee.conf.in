## Server options. Any change made in this section will need a server restart
#to take effect.
[bumblebeed]

# The secondary Xorg server DISPLAY number
VirtualDisplay=@CONF_XDISP@

# Should the unused Xorg server be kept running? Set this to true if waiting
# for X to be ready is too long and don't need power management at all.
KeepUnusedXServer=@CONF_KEEPONEXIT@

# The name of the Bumbleblee server group name (GID name)
ServerGroup=@CONF_GID@

# Card power state at exit. Set to false if the card shoud be ON when Bumblebee
# server exits.
TurnCardOffAtExit=@CONF_TURNOFFATEXIT@

# The default behavior of '-f' option on optirun. If set to "true", '-f' will
# be ignored.
NoEcoModeOverride=false

# The Driver used by Bumblebee server. (See specific sections below)
Driver=@CONF_DRIVER_MODULE@

## Client options. Will take effect on the next optirun executed.
[optirun]

# The method used for VirtualGL to transport frames between X servers.
# Possible values are "proxy", "rgb", "yuv" and "jpeg"
VGLTransport=@CONF_VGLCOMPRESS@

# Should the program run under optirun even if Bumblebee server or nvidia card is not available?
AllowFallbackToIGC=@CONF_FALLBACKSTART@

## Section with nvidia driver specific options. This will be parsed if
## Driver=nvidia
[nvidia]
KernelDriver=@CONF_DRIVER_MODULE@
Module=nvidia
PMMethod=@CONF_PM_METHOD@
LibraryPath=/usr/lib/nvidia-bumblebee
XorgModulePath=/usr/lib/nvidia-bumblebee/xorg/modules
XorgConfFile=/etc/bumblebee/xorg.conf.nvidia

## Section with nouveau driver specific options. This will be parsed if
## Driver=nouveau
[nouveau]
KernelDriver=nouveau
PMMethod=@CONF_PM_METHOD@
XorgConfFile=/etc/bumblebee/xorg.conf.nouveau

